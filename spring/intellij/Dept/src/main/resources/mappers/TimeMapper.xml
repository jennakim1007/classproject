<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.app.manager.mapper.TimeMapper">
    <!--namespace = 만들어둔 인터페이스 경로 풀네임-->

    <select id="getTime" resultType="string">
    <!--id는 실행하고자 하는 메소드 이름-->
        select now()
    </select>

</mapper>